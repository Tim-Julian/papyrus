<?xml version="1.0" encoding="ASCII"?>
<discovery:DiscoveryDefinition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:discovery="http://www.eclipse.org/papyrus/discovery/1.0">
  <categories
      name="Modeler extensions"
      description="Extension of the tool">
    <components
        name="Diagram generation"
        provider="Eclipse Modeling Project"
        description="Offers a tool for generating Papyrus Diagrams from a semantic UML Model"
        license="EPL"
        groups="//@filters.1"
        image32="images/DiagramTemplate.gif"
        incubation="true">
      <sitesURLS>http://download.eclipse.org/modeling/mdt/papyrus/updates/extra/kepler</sitesURLS>
      <overview
          summary="Offers a tool for generating Papyrus Diagrams from a semantic UML Model"/>
      <id>org.eclipse.papyrus.extra.diagramtemplate.feature</id>
    </components>
    <components
        name="CDO Model Repository"
        provider="Eclipse Modeling Project"
        description="Provides integration with CDO model repository technology for sharing and real-time collaborative editing of Papyrus models."
        license="EPL"
        groups="//@filters.1"
        image32="images/Papyrus_CDO.png"
        incubation="true">
      <sitesURLS>http://download.eclipse.org/modeling/mdt/papyrus/updates/extra/kepler</sitesURLS>
      <overview
          summary="Provides integration with CDO model repository technology for sharing and real-time collaborative editing of Papyrus models."/>
      <id>org.eclipse.papyrus.extra.cdo.feature</id>
    </components>
  </categories>
  <filters name="Stable"/>
  <filters name="Experimental"/>
</discovery:DiscoveryDefinition>
